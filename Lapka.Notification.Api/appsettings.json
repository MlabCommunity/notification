{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "",
        "Protocols": "Http1AndHttp2"
      },
      "gRPC": {
        "Url": "",
        "Protocols": "Http2"
      }
    }
  },
  "Mail": { //example for outlook -> overwrite in env variable
    "SenderName": "≈Åapka",
    "Server": "smtp-mail.outlook.com",
    "Port": 587,
    "EnableSsl": true,
    "Username": "lapkaemailaddress@outlook.com",
    "Password": "password"
  },
  "ConnectionStrings": {
    "postgres": ""
  },
  "rabbitMq": {
    "connectionName": "rabbitNotification",
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true
    },
    "username": "rabbitmq",
    "password": "rabbitmq",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      ""
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "notification"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "notification-service/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "maxProducerChannels": 1000,
    "requeueFailedMessages": false,
    "spanContextHeader": "span_context"
  }
}
