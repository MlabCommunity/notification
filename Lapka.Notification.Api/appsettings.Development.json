{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5030",
        "Protocols": "Http1AndHttp2"
      },
      "gRPC": {
        "Url": "http://localhost:5031",
        "Protocols": "Http2"
      }
    }
  },
  "ConnectionStrings": {
    "postgres": "Password=postgres;Host=localhost;Port=5431;Database=postgresNotification;Username=postgres"
  },
  "rabbitMq": {
    "connectionName": "rabbitNotification",
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true
    },
    "username": "rabbitmq",
    "password": "rabbitmq",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "exchange": {
      "name": "notification"
    },
    "queue": {
      "template": "notification-service/{{exchange}}.{{message}}"
    }
  }
}
