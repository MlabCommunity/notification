{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
        "Http": {
          "Url": "http://localhost:5030"
        },
        "gRCP": {
          "Url": "http://localhost:5031"
        }
      }
    },
    "ConnectionStrings": {
      "postgres": "Password=postgres;Host=localhost;Port=5431;Database=postgresNotification;Username=postgres"
    },
    "rabbitMq": {
      "connectionName": "rabbitNotification",
      "retries": 3,
      "retryInterval": 2,
      "conventionsCasing": "snakeCase",
      "logger": {
        "enabled": true
      },
      "username": "rabbitmq",
      "password": "rabbitmq",
      "virtualHost": "/",
      "port": 5435,
      "hostnames": [
        "localhost"
      ],
      "exchange": {
        "name": "notification"
      },
      "queue": {
        "template": "notification-service/{{exchange}}.{{message}}"
      }
    }
  }
