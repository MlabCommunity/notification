{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5010",
        "Protocols": "Http1AndHttp2"
      },
      "gRCP": {
        "Url": "http://localhost:5011",
        "Protocols": "Http2"
      }
    }
  },
  "Postgres": {
    "ConnectionString": "Password=admin;Host=localhost;Port=5432;Database=Lappka.NotificationDB;Username=postgres"
  },

  "rabbitMq": {
    "connectionName": "rabbitmq",
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true
    },
    "username": "myuser",
    "password": "mypassword",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "events"
    },
    "queue": {
      "exclusive": false,
      "template": "message_context"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "maxProducerChannels": 1000,
    "requeueFailedMessages": false,
    "spanContextHeader": "span_context"
  }
}
